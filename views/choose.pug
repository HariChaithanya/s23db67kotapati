extends layout

block content
  h1= title
  - const image_names = ['guitar1.jpg', 'guitar2.jpg', 'guitar3.jpg', 'guitar4.jpg', 'guitar5.jpg']

  - for (let i = 0; i < 3; i++)
    - let randomIndex = Math.floor(Math.random() * 5)
    
    img(
      src=`/images/${image_names[randomIndex]}`,
      alt=`Image ${randomIndex + 1}`,
      height=300,
      width=300
      style="padding: 10px;"
    )